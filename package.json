{
  "name": "aomidro-pigg",
  "version": "0.0.0",
  "description": "chat tool",
  "engines": {
    "node": "6.1.0"
  },
  "main": "app.js",
  "scripts": {
    "start": "babel-node ./index.js",
    "dev": "DEBUG=aomidro-pigg:* npm start",
    "build": "npm run build-css && npm run build-js",
    "lint": "eslint **/*.js",
    "build-css": "postcss -c ./.postcssrc.json",
    "watch-css": "postcss -c ./.postcssrc.json -w --map",
    "build-js": "browserify --extension=jsx public/assets/javascripts/app/app.jsx -t babelify -t uglifyify -o public/assets/javascripts/bundle.js",
    "watch-js": "watchify --debug --extension=jsx public/assets/javascripts/app/app.jsx -t babelify -o public/assets/javascripts/bundle.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AomidroPigg/aomidro-pigg.git"
  },
  "author": "nabeliwo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AomidroPigg/aomidro-pigg/issues"
  },
  "homepage": "https://github.com/AomidroPigg/aomidro-pigg#readme",
  "dependencies": {
    "boom": "^3.2.1",
    "config": "^1.20.4",
    "crypt": "0.0.1",
    "handlebars": "^4.0.5",
    "hapi": "^13.4.1",
    "hapi-auth-jwt2": "^7.0.1",
    "inert": "^4.0.0",
    "jsonwebtoken": "^7.0.0",
    "moment": "^2.13.0",
    "mysql": "^2.10.2",
    "node-uuid": "^1.4.7",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-act": "^0.4.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.4",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6",
    "socketio-jwt": "^4.3.4",
    "vision": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "cssnano": "^3.5.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.2.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
