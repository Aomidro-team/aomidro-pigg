{
  "extends": "airbnb",
  "rules": {
    "no-use-before-define": 0,
    "no-console": 0,
    "new-cap": 0,
    "max-len": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true
  },
  "globals": {
    "expect": true
  }
}
